{
  "name": "food-out-app",
  "version": "0.1.0",
  "description": "An app to plan our food out appointments",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.9",
    "@types/node": "11.10.4",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.5",
    "styled-components": "^4.1.3",
    "typescript": "3.3.3333"
  },
  "devDependencies": {
    "@types/styled-components": "^4.1.12",
    "coveralls": "^3.0.3",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:clean": "rimraf ./coverage",
    "pretest": "npm run test:clean",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "lint:js": "./node_modules/.bin/standard",
    "lint:js:fix": "./node_modules/.bin/standard --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "standard": {
    "ignore": [
      "build/*"
    ],
    "env": [
      "jest",
      "es6",
      "browser"
    ]
  }
}
